<% if current_user  %>
  <div class="google">Welcome, you are now signed in as <%= "#{current_user.first_name} #{current_user.last_name}" %>
    <div class="hollow button"><%= link_to "Logout", logout_path %></div>
  <% else %>
    <div class='google'>
      <div id = "google-sign-in" class="hollow button">
        <%= link_to "Sign in with Google", "/auth/google" %>
      <% end %>
    </div>
  </div>
</div>

